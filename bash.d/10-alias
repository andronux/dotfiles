alias ls="ls --color"
alias ll="ls -Gla"
alias l="ls -1"
alias cls="clear; ls"
alias cll="clear; ll"
alias cl="clear; l"
alias cp="cp -vi"
alias rm="rm -vi"
alias mv="mv -vi"
alias s="sudo"
alias se="sudoedit"
alias grep="grep --line-number --color"
alias gdb="gdb -q"
alias sdig="dig +short"
alias valgrind="valgrind -q"
alias stow="stow -d $HOME/.dotfiles"

alias "vi=vim"
